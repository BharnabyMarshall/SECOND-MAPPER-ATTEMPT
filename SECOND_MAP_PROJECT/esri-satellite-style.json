{
  "version": 8,
  "name": "ESRI Satellite",
  "metadata": {
    "mapbox:autocomposite": false,
    "mapbox:type": "template"
  },
  "sources": {
    "esri-satellite": {
      "type": "raster",
      "tiles": [
        "https://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
      ],
      "tileSize": 256,
      "attribution": "Â© Esri, Maxar, Earthstar Geographics",
      "maxzoom": 19,
      "scheme": "xyz"
    },
    "openmaptiles": {
      "type": "vector",
      "url": "https://api.maptiler.com/tiles/v3/tiles.json?key=mTauVrXF22hFSJJ8MDcT"
    }
  },
  "sprite": "https://api.maptiler.com/maps/streets/sprite",
  "glyphs": "https://api.maptiler.com/fonts/{fontstack}/{range}.pbf?key=mTauVrXF22hFSJJ8MDcT",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#000000"
      }
    },
    {
      "id": "esri-satellite",
      "type": "raster",
      "source": "esri-satellite",
      "minzoom": 0,
      "maxzoom": 19
    },
    {
      "id": "country-borders",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "admin",
      "filter": ["==", "admin_level", 2],
      "paint": {
        "line-color": "#ffffff",
        "line-opacity": 0.7,
        "line-width": 1.5
      }
    },
    {
      "id": "state-borders",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "admin",
      "filter": ["==", "admin_level", 4],
      "paint": {
        "line-color": "#ffffff",
        "line-opacity": 0.5,
        "line-width": 0.8
      }
    }
  ]
}
